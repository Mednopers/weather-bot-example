{
    "name": "mednopers/weather-bot-example",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolay Gordeev",
            "email": "gordeev.ns@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "ext-json": "^1.6",
        "botman/botman": "^2.5",
        "botman/driver-telegram": "^1.6",
        "botman/driver-web": "^1.5",
        "cmfcmf/openweathermap-php-api": "^3.2",
        "guzzlehttp/guzzle": "^7.1",
        "laminas/laminas-config-aggregator": "^1.2",
        "laminas/laminas-diactoros": "^2.2",
        "monolog/monolog": "^2.0",
        "php-di/php-di": "^6.1",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "slim/slim": "^4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    }
